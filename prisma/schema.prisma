generator client {
  provider = "prisma-client-js"
  previewFeatures = []
}

datasource db {
  provider = "postgresql"
  url = env("DATABASE_URL")
}

model ContactSubmission {
  id          Int      @id @default(autoincrement())
  companyName String
  email       String
  message     String
  status      String   @default("pending")
  createdAt   DateTime @default(now())
}

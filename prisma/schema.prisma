generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model ContactSubmission {
  id          Int      @id @default(autoincrement())
  companyName String
  email       String
  message     String
  createdAt   DateTime @default(now())
  status      String   @default("pending")
}
